<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.support.android.designlibdemo">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/Base.Theme.DesignDemo">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity
            android:name=".PetsDetailActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".PublishInAdoptionActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".SearchInAdoptionActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".SearchInAdoptionActivity2"
            android:parentActivityName=".SearchInAdoptionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.support.android.designlibdemo.SearchInAdoptionActivity" />
        </activity>

        <activity
            android:name=".ResultListActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>


        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".LoginUserActivity"
            android:label="@string/title_activity_login_user"
            android:windowSoftInputMode="adjustResize|stateVisible"></activity>
        <activity
            android:name=".PublishInAdoptionActivity2"
            android:label="@string/title_activity_publish_in_adoption2"
            android:parentActivityName=".PublishInAdoptionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.support.android.designlibdemo.PublishInAdoptionActivity" />
        </activity>
        <activity
            android:name=".PublishInAdoptionActivity3"
            android:label="@string/title_activity_publish_in_adoption3"
            android:parentActivityName=".PublishInAdoptionActivity2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.support.android.designlibdemo.PublishInAdoptionActivity2" />
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="RegisterActivity">
        </activity>
        <activity
            android:name=".Register2Activity"
            android:label="RegisterActivity">
        </activity>
    </application>

</manifest>
